    <queryString language="SQL">
		<![CDATA[SELECT pedido.data_pedido,
	pedido.observacoes,
	pedido.desconto,
	pedido.data_pedido,
	cliente.nome,
	cliente.cpf,
	cliente.cnpj,
	cliente.telefone1,
	endereco.logradouro,
	endereco.numero,
	endereco.complemento,
	endereco.bairro
